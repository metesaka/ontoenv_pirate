brick:Building a owl:Class,
        sh:NodeShape ;
    rdfs:label "Building"@en ;
    rdfs:subClassOf brick:Location ;
    owl:deprecated true ;
    skos:definition "An independent unit of the built environment with a characteristic spatial structure, intended to serve at least one function or user activity [ISO 12006-2:2013]"@en ;
    sh:property [ sh:or ( [ sh:class brick:Floor ] [ sh:class brick:Room ] [ sh:class brick:Space ] [ sh:class brick:Zone ] [ sh:class brick:Wing ] ) ;
            sh:path brick:hasPart ],
        <https://brickschema.org/schema/BrickShape#hasAddressShape>,
        <https://brickschema.org/schema/BrickShape#hasbuildingPrimaryFunctionShape>,
        <https://brickschema.org/schema/BrickShape#hasbuildingThermalTransmittanceShape>,
        <https://brickschema.org/schema/BrickShape#hasyearBuiltShape> ;
    sh:rule [ a sh:TripleRule ;
            sh:object <https://brickschema.org/schema/BrickTag#Location> ;
            sh:predicate brick:hasTag ;
            sh:subject sh:this ],
        [ a sh:TripleRule ;
            sh:object <https://brickschema.org/schema/BrickTag#Building> ;
            sh:predicate brick:hasTag ;
            sh:subject sh:this ] ;
    brick:deprecatedInVersion "1.4.0" ;
    brick:deprecationMitigationMessage "Brick location classes are being phased out in favor of RealEstateCore classes. For a replacement, consider rec:Building" ;
    brick:hasAssociatedTag <https://brickschema.org/schema/BrickTag#Building>,
        <https://brickschema.org/schema/BrickTag#Location> ;
    brick:isReplacedBy <https://w3id.org/rec#Building> .

