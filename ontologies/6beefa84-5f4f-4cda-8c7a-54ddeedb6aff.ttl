    rdfs:subClassOf brick:Equipment ;
    sh:property [ sh:or ( [ sh:class brick:Lighting_Equipment ] [ sh:class brick:Electrical_Equipment ] ) ;
            sh:path brick:hasPart ],
        [ sh:or ( [ sh:class brick:Lighting_Equipment ] [ sh:class <https://w3id.org/rec#Space> ] ) ;
            sh:path brick:feeds ] ;
    sh:rule [ a sh:TripleRule ;
            sh:object <https://brickschema.org/schema/BrickTag#Equipment> ;
            sh:predicate brick:hasTag ;
            sh:subject sh:this ],
