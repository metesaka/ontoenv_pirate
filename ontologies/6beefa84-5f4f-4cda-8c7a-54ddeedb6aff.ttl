            sh:name "installed by"^^xsd:string ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/rec#installedBy> ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:date ;
            sh:maxCount 1 ;
            sh:name "turnover date"^^xsd:string ;
            sh:path <https://w3id.org/rec#turnoverDate> ],
        [ a sh:PropertyShape ;
            sh:class <https://w3id.org/rec#Geometry> ;
            sh:description "A GeoJSON Geometry representing the position or extent of the asset."^^xsd:string ;
            sh:maxCount 1 ;
            sh:name "geometry"^^xsd:string ;
            sh:path <https://w3id.org/rec#geometry> ],
        [ a sh:PropertyShape ;
            sh:class <https://w3id.org/rec#Agent> ;
            sh:name "serviced by"^^xsd:string ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/rec#servicedBy> ],
        [ a sh:PropertyShape ;
            sh:class <https://w3id.org/rec#Document> ;
            sh:name "documentation"^^xsd:string ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/rec#documentation> ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "initial cost"^^xsd:string ;
            sh:nodeKind sh:Literal ;
            sh:path <https://w3id.org/rec#initialCost> ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:name "IP address"^^xsd:string ;
            sh:path <https://w3id.org/rec#IPAddress> ],
        [ a sh:PropertyShape ;
            sh:class <https://w3id.org/rec#Asset> ;
            sh:name "is part of"^^xsd:string ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/rec#isPartOf> ],
        [ a sh:PropertyShape ;
            sh:class brick:Point ;
            sh:name "has point"^^xsd:string ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/rec#hasPoint> ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:name "asset tag"^^xsd:string ;
            sh:path <https://w3id.org/rec#assetTag> ],
        [ a sh:PropertyShape ;
            sh:class <https://w3id.org/rec#Agent> ;
            sh:name "commissioned by"^^xsd:string ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/rec#commissionedBy> ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:name "serial number"^^xsd:string ;
            sh:path <https://w3id.org/rec#serialNumber> ],
        [ a sh:PropertyShape ;
            sh:class <https://w3id.org/rec#Asset> ;
            sh:name "has part"^^xsd:string ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/rec#hasPart> ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:name "model number"^^xsd:string ;
            sh:path <https://w3id.org/rec#modelNumber> ],
        [ a sh:PropertyShape ;
