            sh:path <https://w3id.org/rec#commissioningDate> ],
        [ a sh:PropertyShape ;
            sh:class <https://w3id.org/rec#BuildingElement> ;
            sh:description "An asset may be mounted on some part of the building construction (e.g., a blind on a facade, a camera on a wall, etc)."^^xsd:string ;
            sh:maxCount 1 ;
            sh:name "mounted on"^^xsd:string ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/rec#mountedOn> ],
        [ a sh:PropertyShape ;
