    sh:targetObjectsOf <https://brickschema.org/schema/Brick/ref#hasExternalReference> .

<https://brickschema.org/schema/Brick/ref#ExternalReferenceShape> a sh:NodeShape ;
    sh:property [ sh:message "All ExternalReference must have an skos:definition" ;
            sh:minCount 1 ;
            sh:path skos:definition ],
        [ sh:message "All ExternalReference must have an rdfs:label" ;
            sh:minCount 1 ;
            sh:path rdfs:label ] ;
    sh:target [ a sh:SPARQLTarget ;
            sh:prefixes <https://brickschema.org/schema/1.4/Brick> ;
            sh:select """
    SELECT ?this WHERE {
     ?this rdfs:subClassOf+ ref:ExternalReference .
    }
    """ ] .

