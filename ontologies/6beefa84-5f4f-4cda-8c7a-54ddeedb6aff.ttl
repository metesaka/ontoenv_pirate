            sh:path <https://brickschema.org/schema/Brick/ref#hasExternalReference> ;
            sh:qualifiedMaxCount 1 ;
            sh:qualifiedValueShape [ sh:class <https://brickschema.org/schema/Brick/ref#ExternalReference> ;
